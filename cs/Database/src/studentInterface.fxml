<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="557.0" prefWidth="882.0" style="-fx-background-color: #262626;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="studentInterfaceController">
   <children>
      <VBox prefHeight="200.0" prefWidth="297.0">
         <children>
            <Label fx:id="nameLabel" text="Welcome" textFill="#e8e8e8">
               <font>
                  <Font size="29.0" />
               </font></Label>
            <Label fx:id="idLabel" text="ID:" textFill="#e8e8e8" />
            <Label fx:id="deptLabel" text="Department:" textFill="#e8e8e8" />
            <Label fx:id="credLabel" text="Total Credits: " textFill="#e8e8e8" />
         </children>
      </VBox>
      <HBox layoutX="14.0" layoutY="457.0" prefHeight="100.0" prefWidth="675.0">
         <children>
            <Button fx:id="transcriptButton" mnemonicParsing="false" onAction="#handleTranscript" text="Get Transcipt" />
            <Button fx:id="degreeButton" mnemonicParsing="false" onAction="#handleDegree" text="Check Degree Requirements" />
            <Button fx:id="addCourseButton" mnemonicParsing="false" onAction="#handleAdd" text="Add Course" />
            <Button fx:id="removeCourseButton" mnemonicParsing="false" onAction="#handleRemove" text="Remove Course" />
            <Button fx:id="exitProgramButton" mnemonicParsing="false" onAction="#handleExitProgram" text="Exit" />
         </children>
      </HBox>
      <TableView fx:id="currentCourseTable" layoutX="97.0" layoutY="150.0" prefHeight="200.0" prefWidth="566.0" visible="false">
        <columns>
            <TableColumn fx:id="courseIDColumn" prefWidth="109.0" text="Course ID" />
            <TableColumn fx:id="titleColumn" prefWidth="204.0" text="Title" />
            <TableColumn fx:id="semesterColumn" minWidth="0.0" prefWidth="79.0" text="Semester" />
            <TableColumn fx:id="yearColumn" minWidth="0.0" prefWidth="100.0" text="Year" />
            <TableColumn fx:id="gradeColumn" prefWidth="73.0" text="Grade" />
        </columns>
      </TableView>
      <TableView fx:id="missingCourseTable" layoutX="223.0" layoutY="179.0" prefHeight="200.0" prefWidth="384.0" visible="false">
        <columns>
          <TableColumn fx:id="missingID" prefWidth="94.0" text="Course ID" />
          <TableColumn fx:id="missingTitle" prefWidth="269.0" text="Title" />
        </columns>
      </TableView>
      <HBox fx:id="addHBox" layoutX="50.0" layoutY="142.0" prefHeight="244.0" prefWidth="660.0" visible="false">
         <children>
            <VBox prefHeight="244.0" prefWidth="294.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="166.0">
                     <children>
                        <RadioButton fx:id="radio2009" mnemonicParsing="false" selected="true" text="2009">
                           <toggleGroup>
                              <ToggleGroup fx:id="addYear" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="radio2010" mnemonicParsing="false" text="2010" toggleGroup="$addYear" />
                     </children>
                  </HBox>
                  <HBox prefHeight="59.0" prefWidth="294.0">
                     <children>
                        <RadioButton fx:id="radioFall" mnemonicParsing="false" selected="true" text="Fall">
                           <toggleGroup>
                              <ToggleGroup fx:id="addSemester" />
                           </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="radioSpring" mnemonicParsing="false" text="Spring" toggleGroup="$addSemester" />
                        <RadioButton fx:id="radioSummer" mnemonicParsing="false" text="Summer" toggleGroup="$addSemester" />
                     </children>
                  </HBox>
                  <Button fx:id="addSearchCourseButton" mnemonicParsing="false" text="Search Courses" />
                  <Button fx:id="addSelectedClassButton" mnemonicParsing="false" onAction="#handleAddSelected" prefHeight="22.0" prefWidth="180.0" text="Add Selected Course" />
               </children>
            </VBox>
            <TableView fx:id="addCourseTable" prefHeight="244.0" prefWidth="355.0">
              <columns>
                <TableColumn fx:id="addCourseID" prefWidth="109.0" text="Course ID" />
                <TableColumn fx:id="addCourseTitle" prefWidth="224.0" text="Title" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox fx:id="removeHBox" layoutX="72.0" layoutY="125.0" prefHeight="259.0" prefWidth="684.0" visible="false">
         <children>
            <Button fx:id="removeSelectedButton" mnemonicParsing="false" text="Remove Selected Course" />
            <TableView fx:id="removeCourseTable" prefHeight="259.0" prefWidth="440.0">
              <columns>
                <TableColumn fx:id="removeCourseID" prefWidth="98.0" text="Course ID" />
                <TableColumn fx:id="removeCourseTitle" minWidth="0.0" prefWidth="121.0" text="Title" />
                  <TableColumn fx:id="removeCourseSemester" prefWidth="75.0" text="Semester" />
                  <TableColumn fx:id="removeCourseYear" prefWidth="105.0" text="Year" />
              </columns>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
